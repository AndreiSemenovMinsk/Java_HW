import java.util.*;

public class HW0 {
	
private static boolean vozrast(double[] arr){
	
	boolean result = true;
	
	for (int i = 1; i < arr.length; i++){
		if (arr[i] < arr[i-1]){
			result = false;
			break;
		}
	}
	return result;
}

public static void main(String[] args) {

	double[] arr = {1, 12, 6, 23, 24, 30};
	
	System.out.println("Result " + vozrast(arr));
 }
}
